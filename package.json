    "lint-fix-prod": "cross-env NODE_ENV=production eslint --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint-fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
